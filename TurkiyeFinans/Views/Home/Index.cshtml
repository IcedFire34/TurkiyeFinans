@model TurkiyeFinans.Models.ViewModel
@addTagHelper *, Microsoft.AspNetCore.Mvc.TagHelpers
@{
    ViewData["Title"] = "Home Page";
}

<form method="post" asp-controller="Home" asp-action="Index">

    <div class="form-group">
        <label for="exampleInputEmail1">TC NO</label>
        @Html.TextBoxFor(model => model._mernisserviceparametters.TCKimlikNo, new { @class = "form-control", @type = "number", id = "TCKimlikNo" })
    </div>  
    <div class="form-group">
        <label for="exampleInputPassword1">adı</label>
        @Html.TextBoxFor(model => model._mernisserviceparametters.Ad, new { @class = "form-control", @type = "text", id = "Ad" })
    </div>
    <div class="form-group">
        <label for="exampleInputPassword1">Soyadı</label>
        @Html.TextBoxFor(model => model._mernisserviceparametters.Soyad, new { @class = "form-control", @type = "text", id = "Soyadı" })
    </div>
    <div class="form-group">
        <label for="exampleInputPassword1">Dogum Yılı</label>
        @Html.TextBoxFor(model => model._mernisserviceparametters.DogumYili, new { @class = "form-control", @type = "text", id = "DogumYılı" })
    </div>
    <button type="submit" class="btn btn-primary" asp-controller="Home" asp-action="Index">Kontrol Et</button>
</form>
